{
    "name": "Test",
    "description": "",
    "version": "1.0.0",
    "manifest_version": 3,
    
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html"
    },
    "permissions": ["storage","tabs","notifications"],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }]
}